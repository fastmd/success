    <div class="container-fluid">
    	<div class="row">
    		  <nav class="navbar"><ul class="nav nav-pills pull-left"><li></li></ul></nav>
    		  <nav class="navbar"><ul class="nav nav-pills pull-left"><li></li></ul></nav>		    	
	      	  <%= render 'layouts/messages' %>	  
